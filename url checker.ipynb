    "import re\n",
    "\n",
    "user_input = input(\"URL: \").strip()\n",
    "if match:= re.search(r\"^(?:https?://)?(?:www\\.)?(?:[a-z0-9]+)\\.(?:com|net|org|edu)\\/([a-z0-9._]+)\", user_input, re.IGNORECASE):\n",
    "    print(f\"Username: {match.group(1)}\")\n",
    "else:\n",
    "    print(\"Invalid URL\")"
   ]
