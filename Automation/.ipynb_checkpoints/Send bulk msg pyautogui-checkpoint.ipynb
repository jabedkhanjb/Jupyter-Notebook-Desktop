   "source": [
    "import pyautogui as py\n",
    "import time\n",
    "\n",
    "msg = input(\"What's on your mind?\\n> \").strip()\n",
    "msg_limit = int(input(\"How many message would you like to send?\\n> \"))\n",
    "\n",
    "time.sleep(5)\n",
    "i = 0\n",
    "while i<msg_limit:\n",
    "    py.typewrite(msg)\n",
    "    py.press(\"Enter\")\n",
    "    i+=1\n",
    "\n"
   ]
  }
